{
  "name": "shared-store",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/store/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:state"],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/store",
        "main": "libs/store/src/index.ts",
        "tsConfig": "libs/store/tsconfig.lib.json",
        "assets": ["libs/store/*.md"],
        "webpackConfig": "libs/store/webpack.config.js",
        "compiler": "babel"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "generatePackageJson": true
        }
      }
    }
  }
}
