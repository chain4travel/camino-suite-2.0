{
  "name": "store",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/store/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:state"],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/store/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/store/jest.config.ts"
      }
    },
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/store",
        "main": "libs/store/src/index.ts",
        "tsConfig": "libs/store/tsconfig.lib.json",
        "assets": [
          {
            "glob": "*.md",
            "input": "libs/store/",
            "output": "."
          }
        ],
        "webpackConfig": "libs/store/webpack.config.js",
        "target": "web",
        "compiler": "babel",
        "sourceMap": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "generatePackageJson": true
        }
      }
    }
  }
}
